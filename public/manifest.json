{
  "name": "tuner-v3",
  "description": "Chrome extension for hack different page",
  "version": "1.0.107",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "页面配置",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "contextMenus",
    "notifications",
    "declarativeContent",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}