{
  "name": "tuner-v3",
  "description": "Chrome extension for hack different page",
  "homepage_url": "https://www.ohohoho.com",
  "version": "1.0.7728799",
  "manifest_version": 3,
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "action": {
    "default_title": "页面配置",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "hack/**/*.js",
        "hack/**/*.css",
        "hack/*.js",
        "hack/*.css"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "contextMenus",
    "notifications",
    "webNavigation",
    "declarativeContent",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "cdn-rule",
        "enabled": true,
        "path": "./cdn-rule.json"
      }
    ]
  },
  "chrome_url_overrides": {
    "newtab": "navigator.html"
  },
  "host_permissions": [
    "*://*/*"
  ]
}